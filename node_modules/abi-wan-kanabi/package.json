  },
  "homepage": "https://github.com/keep-starknet-strange/abi-wan-kanabi#readme",
  "publishConfig": {
    "ivpavici:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "abi-wan-kanabi": "^1.0.1",
    "fs-extra": "^10.0.0",
    "rome": "^12.1.3",
    "typescript": "^4.9.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
